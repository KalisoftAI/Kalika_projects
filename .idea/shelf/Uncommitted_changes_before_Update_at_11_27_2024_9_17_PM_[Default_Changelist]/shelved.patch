<+><!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>Shopping Cart</title>\r\n    <style>\r\n        body {\r\n            font-family: Arial, sans-serif;\r\n            background-color: #f9f9f9;\r\n            margin: 0;\r\n            padding: 0;\r\n        }\r\n        .cart-container {\r\n            max-width: 800px;\r\n            margin: 50px auto;\r\n            background-color: #fff;\r\n            border-radius: 8px;\r\n            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n            padding: 20px;\r\n        }\r\n        .cart-header {\r\n            text-align: center;\r\n            font-size: 1.5rem;\r\n            font-weight: bold;\r\n            margin-bottom: 20px;\r\n        }\r\n        .cart-item {\r\n            display: flex;\r\n            align-items: center;\r\n            justify-content: space-between;\r\n            padding: 15px;\r\n            border-bottom: 1px solid #ddd;\r\n        }\r\n        .cart-item:last-child {\r\n            border-bottom: none;\r\n        }\r\n        .product-image {\r\n            width: 80px;\r\n            height: 80px;\r\n            border-radius: 8px;\r\n            object-fit: cover;\r\n        }\r\n        .product-info {\r\n            flex: 1;\r\n            margin-left: 20px;\r\n        }\r\n        .product-info strong {\r\n            font-size: 1.2rem;\r\n        }\r\n        .cart-buttons {\r\n            display: flex;\r\n            align-items: center;\r\n            gap: 10px;\r\n        }\r\n        .cart-buttons button {\r\n            background-color: #007bff;\r\n            color: white;\r\n            border: none;\r\n            padding: 8px 10px;\r\n            font-size: 1rem;\r\n            cursor: pointer;\r\n            border-radius: 4px;\r\n            transition: background-color 0.3s;\r\n        }\r\n        .cart-buttons button:hover {\r\n            background-color: #0056b3;\r\n        }\r\n        .cart-buttons .remove-btn {\r\n            background-color: #dc3545;\r\n        }\r\n        .cart-buttons .remove-btn:hover {\r\n            background-color: #c82333;\r\n        }\r\n        .total {\r\n            text-align: right;\r\n            font-size: 1.5rem;\r\n            font-weight: bold;\r\n            margin-top: 20px;\r\n        }\r\n        .btn {\r\n            background-color: #007BFF;\r\n            color: white;\r\n            border: none;\r\n            padding: 10px 15px;\r\n            border-radius: 5px;\r\n            cursor: pointer;\r\n        }\r\n        .btn:hover {\r\n            background-color: #0056b3;\r\n        }\r\n        /* Responsive Design */\r\n        @media (max-width: 600px) {\r\n            .cart-item {\r\n                flex-direction: column;\r\n                align-items: flex-start;\r\n            }\r\n            .product-image {\r\n                width: 100%;\r\n                height: auto;\r\n                margin-bottom: 10px;\r\n            }\r\n            .product-info {\r\n                margin-left: 0;\r\n            }\r\n        }\r\n    </style>\r\n</head>\r\n<body>\r\n\r\n<div class=\"cart-container\">\r\n    <div class=\"cart-header\">Your Shopping Cart</div>\r\n    <div id=\"cartItems\">\r\n        {% if cart_items and cart_items|length > 0 %}\r\n        <form method=\"POST\" action=\"{{ url_for('check.checkout') }}\">\r\n            {% for item in cart_items %}\r\n            <div class=\"cart-item\" data-item-id=\"{{ loop.index }}\">\r\n                <img src=\"/images/{{ item.name.replace(' ', '_')|lower }}.jpg\" alt=\"{{ item.name }}\" class=\"product-image\">\r\n                <div class=\"product-info\">\r\n                    <strong>{{ item.name }}</strong>\r\n                    <p>Quantity: <span class=\"item-quantity\">{{ item.quantity }}</span></p>\r\n                    <p>Price: RS.{{ item.price }}</p>\r\n                    <p>Total: RS.<span class=\"item-total\">{{ item.quantity * item.price }}</span></p>\r\n                </div>\r\n                <div class=\"cart-buttons\">\r\n                    <button class=\"increase-btn\">+</button>\r\n                    <button class=\"decrease-btn\">-</button>\r\n                    <button class=\"remove-btn\">Remove</button>\r\n                </div>\r\n            </div>\r\n            {% endfor %}\r\n            <div class=\"checkout-details\">\r\n                <label for=\"user_name\">Name:</label>\r\n                <input type=\"text\" id=\"user_name\" name=\"user_name\" required>\r\n\r\n                <label for=\"shipping_address\">Shipping Address:</label>\r\n                <input id=\"shipping_address\" name=\"shipping_address\" required>\r\n\r\n                <label for=\"payment_status\">Payment Status:</label>\r\n                <select id=\"payment_status\" name=\"payment_status\" required>\r\n                    <option value=\"paid\">Paid</option>\r\n                    <option value=\"pending\">Pending</option>\r\n                </select>\r\n\r\n                <input type=\"hidden\" name=\"total_amount\" value=\"{{ cart_items|sum(attribute='price') }}\">\r\n            </div>\r\n\r\n            <div class=\"total\">Total: RS. <span id=\"totalAmountValue\">{{ cart_items|sum(attribute='price') }}</span></div>\r\n            <button type=\"submit\" class=\"btn\">Checkout</button>\r\n        </form>\r\n        {% else %}\r\n            <p>Your cart is empty.</p>\r\n        {% endif %}\r\n    </div>\r\n</div>\r\n\r\n<script>\r\n   document.querySelector('.product-grid').addEventListener('click', event => {\r\n    if (event.target.classList.contains('add-to-cart')) {\r\n        event.preventDefault(); // Prevent default button behavior\r\n\r\n        // Find the parent form for the clicked button\r\n        const productForm = event.target.closest('.product-form');\r\n        if (!productForm) {\r\n            console.error(\"Form not found for the product!\");\r\n            return;\r\n        }\r\n\r\n        // Collect form data\r\n        const formData = new FormData(productForm);\r\n\r\n        // Debugging: Log data being sent to the server\r\n        console.log(\"Product data being sent to server:\", Object.fromEntries(formData));\r\n\r\n        // Send the data to the backend\r\n        fetch(productForm.action, {\r\n            method: 'POST',\r\n            body: formData\r\n        })\r\n        .then(response => {\r\n            if (response.ok) {\r\n                window.location.href = '{{ url_for(\"cart1.cart\") }}'; // Redirect to cart page\r\n            } else {\r\n                alert('Failed to add product to cart.');\r\n            }\r\n        })\r\n        .catch(error => {\r\n            console.error('Error sending product data:', error);\r\n        });\r\n    }\r\n});\r\nfunction updateCartTotal() {\r\n        const totals = Array.from(document.querySelectorAll('.item-total')).map(el => parseFloat(el.textContent));\r\n        const totalAmount = totals.reduce((sum, current) => sum + current, 0);\r\n\r\n        document.getElementById('totalAmountValue').textContent = totalAmount.toFixed(2);\r\n        document.getElementById('totalAmountInput').value = totalAmount.toFixed(2);\r\n    }\r\n\r\n</script>\r\n\r\n</body>\r\n</html>\r\n
